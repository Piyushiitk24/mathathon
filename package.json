{
  "name": "mathathon-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "install:all": "npm install",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "npm start --workspace=client",
    "dev:server": "npm run dev --workspace=server",
    "build": "npm run build --workspace=client"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}